{
  "name": "rotatable-array",
  "version": "1.3.1",
  "description": "Allows you to rotate/iterate through an array in a circular manner.",
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --fix --type-aware",
    "check:types": "tsgo --noEmit",
    "check": "npm run lint && npm run check:types",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "rotatable",
    "array",
    "circular",
    "iterator"
  ],
  "author": "VastBlast",
  "license": "ISC",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/node": "^24.10.3",
    "@typescript/native-preview": "^7.0.0-dev.20251223.1",
    "oxlint": "^1.35.0",
    "oxlint-tsgolint": "^0.10.0",
    "tsdown": "^0.18.3",
    "typescript": "^5.8.3"
  },
  "type": "module",
  "dependencies": {
    "vitest": "^4.0.15"
  }
}
